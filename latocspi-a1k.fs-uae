#!/usr/bin/env fs-uae

[fs-uae]
accuracy = 1
amiga_model = A1000
floppies_dir = $CONFIG/
floppy_drive_count = 1
floppy_drive_0 = latocspi-a1k.adf
floppy_image_0 = latocspi-a1k.adf
governor_warning = 0
kickstart_file = amiga-boot-a1000.rom
ntsc_mode = 0
save_states = 0
save_states_dir = $CONFIG/
screenshots_output_dir = $CONFIG/
screenshots_output_mask = 2
screenshots_output_prefix = latocspi-a1k
theme = led-bars-edge
zoom = full
